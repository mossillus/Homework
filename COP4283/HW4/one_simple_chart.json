{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Blue Dragon Item Drops from Old School Runescape",
  "data": {
    "url": "https://raw.githubusercontent.com/mossillus/Homework/refs/heads/main/COP4283/HW4/Cleaned%20Blue%20Dragon%20Drops",
    "format": {"type": "csv"}
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "Table Title",
      "type": "nominal",
      "title": "Drop Table"
    },
    "y": {
      "aggregate": "count",
      "type": "quantitative",
      "title": "Number of Drops"
    },
    "color": {
      "field": "Price Tiered",
      "type": "nominal",
      "title": "Price Tier"
    },
    "order": {
      "field": "Price Tiered",
      "sort": "ascending"}
  }
}
